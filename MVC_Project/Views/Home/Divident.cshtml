@model IEnumerable<MVC_Project.Models.Divident>
@{
    ViewData["Title"] = "Divident";
}

<h2>Divident</h2>

<p>
    <a asp-action="Create" asp-controller="">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Payment_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Record_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.type)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Payment_date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Record_date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Amount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.type)
                </td>
            </tr>
        }
    </tbody>
</table>
